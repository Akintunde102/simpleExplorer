  "scripts": {
    "build": "webpack --config webpack.prod.js --env.production --colors --watch",
    "lint:ts": "tslint -p ./tsconfig.json -t stylish",
    "lint:css": "stylelint \"src/**/*.css\"",
    "dev": "webpack --config webpack.dev.js --colors --watch",
    "simplex": "./node_modules/.bin/electron .",
    "simplex:debug": "./node_modules/.bin/electron . --enable-logging --devtools",
    "modules:rebuild": "electron-rebuild -f",
    "package:lmw": "build -lmw --ia32 --x64",
    "package:l": "build -l --ia32 --x64",
    "package:w": "build -w --ia32 --x64",
    "package:m": "build -m"
